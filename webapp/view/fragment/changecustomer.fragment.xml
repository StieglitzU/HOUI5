<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:f="sap.ui.layout.form">

		<f:SimpleForm id="SimpleFormChange354"
			editable="true"
			layout="ResponsiveGridLayout"
			title="{i18n>appTitle}"
			labelSpanXL="3"
			labelSpanL="3"
			labelSpanM="3"
			labelSpanS="12"
			adjustLabelSpan="false"
			emptySpanXL="4"
			emptySpanL="4"
			emptySpanM="4"
			emptySpanS="0"
			columnsXL="1"
			columnsL="1"
			columnsM="1"
			singleContainerFullSize="false" >
			<f:content>
                    <Label id="_edit_label_customerid" text="{i18n>customerid}" labelFor="_edit_text_customerid"></Label>
                    <Input id="_edit_input_customerid" value="{Customerid}"></Input>                      
                    <Label id="_edit_label_firstname" text="{i18n>firstname}" labelFor="_edit_input_firstname"></Label>
                    <Input id="_edit_input_firstname" value="{Firstname}"></Input>                      
                    <Label id="_edit_label_lastname" text="{i18n>lastname}" labelFor="_edit_input_lastname"></Label>
                    <Input id="_edit_input_lastname" value="{Lastname}"></Input>                      
                    <Label id="_edit_label_gender" text="{i18n>gender}" labelFor="_edit_select_gender"></Label>

                    <Select id="_edit_select_gender" items="{genderModel>/genders}" selectedKey="{/gender}">
                        <items>
                             <core:Item id="_edit_selectitem_model" key="{genderModel>}" text="{path: 'genderModel>', formatter: '.genderFormatter'}"/>
                        </items>
                    </Select>

                <!--    
                    <Select id="_edit_select_gender" items="{genderModel>/genders}" selectedKey="{/gender}">
                        <items>
                             <core:Item id="_edit_selectitem_model" key="{genderModel>}" text="{= ${genderModel>}==='female' ?${i18n>female} : ${i18n>male}}"/>
                        </items>
                    </Select>
                 -->  
                    <!--  
                        <Select id="_edit_select_gender">
                           <items>
                                 <core:Item id="_edit_selectitem_male" key="male" text="male"/>
                                 <core:Item id="_edit_selectitem_female" key="female" text="female"/>
                                 <core:Item id="_edit_selectitem_other" key="other" text="other"/>
                             </items>
                         </Select>
                     -->
                    <Label id="_edit_label_title" text="{i18n>title}" labelFor="_edit_input_title"></Label>
                    <Input id="_edit_input_title" value="{Academictitle}"></Input>                    
                    <Label id="_edit_label_mail" text="{i18n>mail}" labelFor="_edit_input_mail"></Label>
                    <Input id="_edit_input_mail" value="{Mail}"></Input>                      
                    <Label id="_edit_label_phone" text="{i18n>phone}" labelFor="_edit_input_phone"></Label>
                    <Input id="_edit_input_phone" value="{Phone}"></Input>    
                    <Label id="_edit_label_homepage" text="{i18n>website}" labelFor="_edit_input_homepage"></Label>
                    <Input id="_edit_input_homepage" value="{Website}"></Input>
			</f:content>
		</f:SimpleForm>       
